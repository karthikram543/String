public class Main
{
	public static void main(String[] args) {
    
    String name="karthik is my";
    int count=0;
    int ch='k';
    
    
    for(int i=0;i<name.length();i++){
        
        if(name.charAt(i)== ch){
            count++;
            
            
        }}
        System.out.println(count);

}}
   
