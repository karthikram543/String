public class javas {

	public static void main (String[]args) {
		
		
		String word="karthik";
		for(int i=0; i<word.length();i++)
		{
		if(i%2==0)	
			System.out.print((char)(word.charAt(i)-32));
		else
			System.out.print(word.charAt(i));
		}
			
		
	}
			
		

}	
			
		
    
    output:
    KaRtHiK
