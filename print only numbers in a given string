public static void main(String[] args) {
		  String s = "Karthik@0-347";


          char[] sarr = s.toCharArray();


          for (int i = 0; i < sarr.length; i++) {


                  if(sarr[i]>=48 && sarr[i]<=57){


                          System.out.print(sarr[i]+", ");


                  }else{


                          //System.out.print(sarr[i]+", ");


                  }


          }


  }

	

}
